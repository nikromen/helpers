#! /bin/bash

. "@eimgdatadir@"/lib

where=$1

pkgs="
  libselinux-python
  yum
"


case $eimg_distro in
  fedora-23-*|fedora-rawhide-*)
    @bindir@/eimg-wait "$where" || exit 1
    eimg_pkg_manager
    @insecure_ssh@ root@@pi_exp_host@ $eimg_pkg_manager_result \
        -y --setopt=tsflags=nodocs install $pkgs \
        || exit 1
    @libexecdir@/eimg-stop || exit 1
    ;;
esac
